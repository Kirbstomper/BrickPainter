{
  "name": "brick-paint",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node server.js",
    "build": "npm run css-build && ng build --prod",
    "build_dev": "npm run css-build && ng build",
    "build_CI": "ng build --prod --base-href https://kirbstomper.github.io/BrickPainter/",
    "test": "ng test",
    "lint": "ng lint",
    "fix-lint": "ng lint --fix=true",
    "e2e": "ng e2e",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "startCss": "npm run css-watch",
    "test_headless": "ng test --watch false --browsers ChromeHeadless",
    "e2e_headless": "ng e2e --protractor-config=e2e/protractor.conf-ci.js",
    "run_tests": "ng lint && npm run test_headless && npm run e2e_headless",
    "heroku-postbuild": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "angular-cli-ghpages": "^1.0.5",
    "body-parser": "^1.20.1",
    "bulma": "^0.7.5",
    "core-js": "^2.5.4",
    "express": "^4.18.2",
    "mongodb": "^3.3.2",
    "node-sass": "^4.12.0",
    "request": "^2.88.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
