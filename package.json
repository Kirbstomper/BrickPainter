{
  "name": "brick-paint",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node server.js",
    "build": "npm run css-build && ng build --prod",
    "build_dev": "npm run css-build && ng build",
    "build_prod": "npm run css-build && ng build --prod --base-href https://kirbstomper.github.io/BrickPainter/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "startCss": "npm run css-watch",
    "test_headless": "ng test --watch false --browsers ChromeHeadless",
    "e2e_headless": "ng e2e --protractor-config=e2e/protractor.conf-ci.js",
    "run_tests": "ng lint && npm run test_headless && npm run e2e_headless",
    "heroku-postbuild": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular-cli-ghpages": "^0.5.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.7.5",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "mongodb": "^3.3.2",
    "node-sass": "^4.12.0",
    "request": "^2.88.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
